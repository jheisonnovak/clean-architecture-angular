# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
tab_width = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = auto

[*.ts]
quote_type = unset
ij_typescript_use_double_quotes = unset

[*.md]
max_line_length = off
trim_trailing_whitespace = false
